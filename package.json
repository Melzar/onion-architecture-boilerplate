{
  "name": "onion-api",
  "version": "0.0.0",
  "private": true,
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "build": "ttsc -p tsconfig.json",
    "serve": "yarn build && node dist/index.js",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "nodemon",
    "lint": "ttsc && eslint '*/**/*.ts'",
    "test": "DOTENV_CONFIG_PATH=\".env.test\" mocha-parallel-tests --config test/.mocharc.json --max-parallel 5",
    "test:mutate": "DOTENV_CONFIG_PATH=\".env.test\" stryker run",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "db:seed": "ts-node -r tsconfig-paths/register -r dotenv/config node_modules/typeorm-seeding/dist/cli.js seed --config ormconfig.js default",
    "db:reload": "TS_NODE_COMPILER_OPTIONS='{\\\"noImplicitAny\\\": false}' ts-node -r dotenv/config -r ts-node/register src/infrastructure/database/cli/dbReload.ts reload",
    "db:migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "db:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.9",
    "@types/helmet": "^0.0.45",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/method-override": "^0.0.31",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.5.0",
    "@types/swagger-ui-express": "^4.1.1",
    "@types/yargs": "^15.0.1",
    "@wufe/mapper": "^0.5.0",
    "bcrypt": "^3.0.4-napi",
    "cors": "^2.8.5",
    "debug": "~4.1.1",
    "dotenv": "^8.2.0",
    "express": "~4.17.1",
    "faker": "^4.1.0",
    "helmet": "^3.21.2",
    "http-status-codes": "^1.4.0",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "pg": "^7.17.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.1.3",
    "typeorm": "^0.2.22",
    "typeorm-seeding": "betiol/typeorm-seeding",
    "typeorm-transactional-cls-hooked": "^0.1.12",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^2.5.0",
    "@stryker-mutator/mocha-framework": "^2.5.0",
    "@stryker-mutator/mocha-runner": "^2.5.0",
    "@stryker-mutator/typescript": "^2.5.0",
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/sinon": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "@zerollup/ts-transform-paths": "^1.7.11",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-typescript-sort-keys": "^0.5.0",
    "husky": "^3.1.0",
    "lint-staged": "^7.3.0",
    "mocha": "^7.0.1",
    "mocha-parallel-tests": "^2.2.2",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "pg-promise": "^10.4.2",
    "prettier": "^1.19.1",
    "sinon": "^8.1.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.10",
    "typescript": "^3.7.5"
  }
}
